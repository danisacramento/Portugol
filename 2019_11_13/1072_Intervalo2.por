programa
{
	
	funcao inicio()
	{
		inteiro x 
		leia (x)
		se (x >= 10 e x <= 20)
		{ 
			escreva("dentro do intervalo")
		}
		senao
		{
			escreva ("fora do intervalo\n")
		} 
	}
}

